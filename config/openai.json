{
  "provider": "openai",
  "apiKey": "your-claudeai-api-key",
  "configuration": {
    "organization": "organization-id",  // Optional
    "basePath": "https://api.openai.com/v1",  // Optional
    "defaultHeaders": {
      "Custom-Header": "value"  // Optional
    }
  },
  "defaultRequestOptions": {
    "model": "gpt-3.5-turbo",
    "temperature": 0.7,
    "max_tokens": 150,
    "top_p": 1.0,
    "frequency_penalty": 0.0,
    "presence_penalty": 0.0,
    "stop": ["\n"]
  }
}
